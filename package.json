{
  "name": "messai-mvp",
  "version": "0.1.0",
  "description": "MESSAi - Advanced platform for microbial fuel cell research with 3D modeling and AI predictions",
  "author": "MESSAi Team <team@messai.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/messai-mvp.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/messai-mvp/issues"
  },
  "homepage": "https://github.com/your-org/messai-mvp#readme",
  "keywords": [
    "microbial-fuel-cell",
    "mfc",
    "bioelectrochemistry",
    "3d-visualization",
    "ai-predictions",
    "research-platform",
    "sustainability",
    "renewable-energy",
    "wastewater-treatment",
    "nextjs",
    "threejs",
    "typescript"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "private": true,
  "scripts": {
    "dev": "next dev -p 3003",
    "build": "prisma generate && next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:all": "node scripts/test-all.js",
    "test:integration": "vitest tests/integration/",
    "test:regression": "vitest tests/regression/",
    "test:critical": "vitest tests/integration/ tests/regression/ tests/components/ParameterForm.integration.test.tsx tests/api/",
    "test:ci": "vitest run --coverage --reporter=json --outputFile=test-results.json",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "prepare": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "clean": "rm -rf .next node_modules",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^5.22.0",
    "@react-three/drei": "^9.117.3",
    "@react-three/fiber": "^8.17.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/nodemailer": "^6.4.17",
    "@types/three": "^0.169.0",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.0",
    "lucide-react": "^0.522.0",
    "next": "^15.3.5",
    "next-auth": "^4.24.11",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.10.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.59.0",
    "recharts": "^2.15.4",
    "tailwind-merge": "^3.3.1",
    "three": "^0.169.0",
    "zod": "^3.25.72",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.4",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.5",
    "jest-environment-jsdom": "^30.0.4",
    "msw": "^2.10.2",
    "postcss": "^8.5.6",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
